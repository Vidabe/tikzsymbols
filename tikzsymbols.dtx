  {
    \bool_if:NTF \l_@@_if_opt_draft_bool
      { \@@_Basic_Tree_draft:nnnnn }
      {
        \bool_if:NTF \g_@@_if_opt_tree_print_bool
          { \@@_Basic_Tree_on:nnnnn }
          { \@@_Basic_Tree_off:nnnnn }
      }
  }
